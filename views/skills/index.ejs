<%- include('../partials/header') %>

<h1>Skills as of <%= time %></h1>
<ul>
  <% skills.forEach(function(s) { %>
    <li>
      <a href="/skills/<%= s.id %>">
        <%= s.skill %>
          - 
        <%= s.mastered ? 'Yes' : 'No' %>
      </a>
    </li>
  <% }); %>
</ul>

<%- include('../partials/footer') %>